<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.example.ota">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ota1_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ota1_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.OTA">
        <activity android:name=".StudentActivity"></activity>
        <activity android:name=".ParentsActivity"></activity>
        <activity android:name=".AdministratorActivity"></activity>
        <activity android:name=".TeacherActivity"></activity>
        <activity android:name=".AttendanceActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>