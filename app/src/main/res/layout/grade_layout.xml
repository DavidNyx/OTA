                android:layout_column="1"
                android:gravity="center"
                android:padding="15dp"
                android:text="giuaki"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/cuoiki_tv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_column="1"
                android:gravity="center"
                android:padding="15dp"
                android:text="cuoiky"
                android:textSize="16sp" />
        </TableRow>
    </TableLayout>
</LinearLayout>